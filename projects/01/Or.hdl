 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
		Nand(a=a, b=a, out=tmp1);
		Nand(a=b, b=b, out=tmp2);
		Nand(a=tmp1, b=tmp2, out=out);
	/*	Or(a=out1, b=out2, out=out);*/
}
